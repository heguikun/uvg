<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.uvgouapp.ui.user.CompleteActivity">

    <TextView
        android:id="@+id/tv_complete"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:padding="@dimen/dp_10"
        android:text="完成"
        android:textColor="@color/app_color"
        android:textSize="@dimen/sp_15" />

    <TextView
        android:id="@+id/tv_datum"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerInParent="true"
        android:text="是否继续前往填写详细资料?"
        android:textColor="@color/app_color"
        android:textSize="@dimen/sp_15" />

    <TextView
        android:id="@+id/tv_audit"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@id/tv_datum"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="@dimen/dp_19"
        android:text="工作人员将会在24小时内审核完毕"
        android:textColor="@color/color_999999"
        android:textSize="@dimen/sp_12" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@id/tv_audit"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="@dimen/dp_8"
        android:drawableTop="@drawable/sq_icon_tb"
        android:drawablePadding="@dimen/dp_12"
        android:text="已申请，等待审核"
        android:textColor="@color/color_333333"
        android:textSize="@dimen/sp_16" />

    <TextView
        android:id="@+id/tv_contact_service"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="@dimen/dp_45"
        android:drawableEnd="@drawable/sq_icon_lxkf"
        android:drawablePadding="@dimen/dp_4"
        android:text="联系客服"
        android:textColor="@color/app_color"
        android:textSize="@dimen/sp_12" />

</RelativeLayout>